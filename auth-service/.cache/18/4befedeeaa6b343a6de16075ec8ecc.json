{\n      success: true\n    });\n\n  } catch (e) {\n    console.log(e);\n    return res.send({\n      success: false,\n      error: { code: 1, msg: 'INTERNAL ERROR' }\n    });\n  }\n\n}\n\nexport default askRecover;"},"lineCount":null}},"error":null,"hash":"50eb1cca6ff9a4ac368ad98d3479a6c5","cacheData":{"env":{}}}